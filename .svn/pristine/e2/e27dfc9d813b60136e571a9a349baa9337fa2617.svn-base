a{text-decoration: none; color: inherit;}
#app{ color: #666666;}
*{
  margin: 0;
  padding: 0;
  box-sizing:border-box;
  border: 0;
  outline: none;
  -webkit-tap-highlight-color:rgba(0,0,0,0);
}
html,body{-webkit-text-size-adjust: 100%;-webkit-tap-highlight-color: rgba(0, 0, 0, 0); height: 100%;}
body{
  -webkit-overflow-scrolling: touch;
  overflow-scrolling: touch;
}
ul,li{
  list-style: none;
  padding:0;
  margin:0;
}
.opacity {
  opacity: 0.5;
}
button{
  background: none;
  cursor: pointer;
}
/* 设置滚动条的样式 */
::-webkit-scrollbar {
  width: 8px;
}
/* 滚动条滑块 */
::-webkit-scrollbar-thumb {
  border-radius: 6px;
  background: rgba(0,0,0,0.1);
}
img{
  vertical-align: middle;
}
button, a, a:active,a:hover,a:focus,a:visited{
  text-decoration: none;
  outline: none;
  border: none;
}
em, i, b, sub, sup {
  font-style: normal;
}
button:focus {
  outline: none;
}
input[type='number'],select{
  -webkit-appearance: none;
}
input:focus, textarea:focus, select:focus {
  outline: none;
}
/* 取消input自动填充背景色 */
input:-webkit-autofill {
  -webkit-box-shadow: 0 0 0px 1000px white inset;
  // border: 1px solid #CCC;
  // border: 1px solid #CCC!important;
}
a,button,input,textarea{
  -webkit-tap-highlight-color: rgba(0,0,0,0)
}
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
  -webkit-appearance: none;
}
input[type="number"]{
  -moz-appearance: textfield;
}
.css{
  /*设置内嵌的元素在 3D 空间如何呈现：保留 3D*/
  -webkit-transform-style: preserve-3d;
  /*（设置进行转换的元素的背面在面对用户时是否可见：隐藏）*/
  -webkit-backface-visibility: hidden;
}

body{
  -webkit-font-smoothing: antialiased;
  line-height: 1.6;
}
a {
  color: #7e8c8d;
  text-decoration: none;
  -webkit-backface-visibility: hidden;
}
.orange {
  color:orange;
}
.username-css{
  color:#999;
  font-size: 12px;
}
.posRela{
  position: relative;
}
.set-tip{
  width: 10px;
  height: 10px;
  border-radius: 50%;
  background-color: #F23C34;
  margin: 7px 0 0 -2px;
}
/* 文字溢出 */
.long-text{
  overflow: hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}
.text-center {
  text-align: center;
}
.color-gray{
  color:gray;
}
.color-phone {
  color:#556F96;
}
.point{
  cursor: pointer;
}
// 文本换行
.break-all {
  word-break: break-all;
  word-wrap: break-word;
}
/* 弹性布局 */
.flex-shrink0 {
  flex-shrink: 0;
}
.flex {
  display: -ms-flexbox;
  display: -webkit-flex; /* Safari */
  display: flex;
  align-items: center;
}
.inline-flex {
  display: -ms-flexbox;
  display: -webkit-inline-flex; /* Safari */
  display: inline-flex;
  align-items: center;
}
.flex-warp,.flex-wrap{
  flex-wrap: wrap;
}
.flex-center {
  display: -ms-flexbox;
  display: -webkit-flex; /* Safari */
  display: flex;
  justify-content: center;
  align-items: center;
}
.flex-between {
  display: -ms-flexbox;
  display: -webkit-flex; /* Safari */
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.flex-start {
  display: -ms-flexbox;
  display: -webkit-flex; /* Safari */
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.flex-around {
  display: -ms-flexbox;
  display: -webkit-flex; /* Safari */
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.flex-column {
  display: -ms-flexbox;
  display: -webkit-flex; /* Safari */
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.flex-column-center {
  display: -ms-flexbox;
  display: -webkit-flex; /* Safari */
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.flex-1{
  flex: 1;
}

.wechat-home{
  width: 100%;
  height: 100%;
  overflow-y: hidden;
  background-image: url('../assets/weChat/chat_bg.png');
  background-size: cover;
}
.chat-content {
  display: flex;
  position: absolute;
  left: 150px;
  top: 150px;
  right: 0;
  bottom: 0;
  //margin: auto;
  width: 860px;
  height: 650px;
  //background-color: #fff;
  box-shadow: 0 0 8px rgba(0,0,0,.1);
  border-radius: 4px;
  vertical-align: inherit;
  .sidebar {
    width: 60px;
    height: 650px;
    background-color: #2b2c2f;
    border-radius: 4px 0 0 4px;
  }
  .chat-right {
    flex: 1;
    height: 650px;
    background: #F5F5F5;
  }

}
.blackColor{
  color: #000000;
}
// 聊天记录窗口
.user-chat-message{
  position: relative;
  width: 100%;
  height: 500px;
  .header {
    height: 60px;
    padding: 15px 0 0 30px;
    box-sizing: border-box;
    border-bottom: 1px solid #e7e7e7;
    .friendname {
      font-size: 18px;
      color: #333;

    }
    .chat-more-icon {
      margin-right: 15px;
      width: 18px;
      height: 16px;
      background: url('../assets/weChat/chat_more.png') center center no-repeat;
      cursor: pointer;
    }
  }
  .have-people-findme {
    position: absolute;
    top: 60px;
    left:0;
    width: 100%;
    background-color: rgba(186,218,248,.9);

    padding: 3px 30px 3px 12px;
    z-index: 100;
    font-size: 14px;
    color:#333;
    > div {
      min-height: 28px;
      line-height: 20px;
      border-left: 3px solid #CA8137;
      padding-left: 5px;
      word-break: break-all;
      word-wrap: break-word;

      span {
        color: #CA8137;
      }
    }
    img {
      width: 20px;
      position: absolute;
      right: 8px;
      top: 25%;
    }
  }
  .message-wrapper {
    position: relative;
    min-height: 440px;
    max-height: 440px;
    padding: 10px 15px;
    box-sizing: border-box;
    overflow-y: auto;
    border-bottom: 1px solid #e7e7e7;
    background-color: #eee;

    &.on {
      min-height: 590px;
      max-height: 590px;
    }

    .more-history {
      text-align: center;
      color:#12B7F5;
      cursor: pointer;
    }
    .message {
      margin-bottom: 15px
    }
    .time{
      width: 100%;
      font-size: 13px;
      margin: 0 auto;
      padding: 7px 0;
      text-align: center;
      span {
        display: inline-block;
        padding: 7px 6px;
        border-radius: 3px;
        color: #888;
      }
    }
    .main {
      position: relative;
      .chat-username {
        position: absolute;
        top: -4px;
        left: 67px;
        font-size: 13px;
      }
      > div {
        margin-top: 15px;
      }
      .handleConfig{
        //display: none;
        z-index: 999;
        box-sizing: border-box;
        position: absolute;
        margin-top: 0;
        top:30px;
        left: 40px;
        width: 50px;
        text-align: center;
        //height:20px;
        background: #fff;
        box-shadow: 0 0 1px rgba(0,0,0,0.5);
        ul {
          overflow: hidden;
          border-radius: 0 5px 5px 5px;
          li{
            padding: 5px;
            cursor: pointer;
            &:hover{
              color: red;
              background-color: #eee;
            }
            &:first-of-type{
              margin-top: 0;
            }
          }
        }
      }
      .message-tuisong {
        display: block;
        width: 350px;
        font-size: 14px;
        text-align: center;
        background: rgba(50, 50, 50, .1);
        border-radius: 15px;
        margin: 0 auto;
        padding: 3px;
        .red-msg {
          color:#fc701a;
        }
      }
      .avatar{
        float: left;
        margin-left: 15px;
        border-radius: 3px;
        margin-right: 5px;
      }
      .chat-content-dom {
        position: relative;
        .chat-loading {
          font-size: 14px;
          position: absolute;
          right: -20px;
          top:13%;
          &.error {
            color:#F56C6C;
          }
          &.success {
            color:#67c23a;
          }
        }
      }
      .resendMsg{
        position: absolute;
        right: -22px;
        top: 8px;
      }
      .content {
        display: inline-block;
        margin-left: 10px;
        position: relative;
        padding: 6px 10px;
        max-width: 330px;
        min-height: 36px;
        line-height: 24px;
        box-sizing: border-box;
        font-size: 14px;
        text-align: left;
        word-break: break-all;
        word-wrap: break-word;
        background-color: #fff;
        border-radius: 4px;
        color:#333;

        &:before{
          content: " ";
          position: absolute;
          top: 12px;
          right: 100%;
          border: 6px solid transparent;
          border-right-color: #fff;
        }
      }
      .chat-img {
        display: inline-block;
        margin-left: 10px;
        background-color: #fff;
        border-radius: 5px;
        img {
          width: 100px;
          height: 100px;
          border-radius: 5px;
        }
      }
      .chat-record,.chat-order {
        display: inline-block;
        .title_p{
          padding: 0 10px;
          color:#000;
        }
        .records {
          width: 280px;
          height: 50px;
          line-height: 50px;
          background-color: #ff0000;
          text-align: center;
          border-radius: 25px;
          background-image: url("../assets/weChat/share_zj.png");
          background-size:100% 100%;
          color:#fff;
          i {
            color:#eeff66;
          }
        }
        .orders {
          width: 280px;
          height: 120px;
          overflow: hidden;
          border-radius: 5px;
          border: 1px solid #f9ad17;
          text-align: center;
          .orders_title{
            background-color: #f9ad17;
            line-height: 30px;
            color:#333;
            font-size: 15px;
          }
          .orders_content {
            background-color: #fff6f6;
            height: 90px;
            text-align: center;
            p{
              line-height: 30px;
              border-bottom:1px solid #f4d4d4 ;
            }
            .orders_c_one {
              width: 80px;
              border-right:1px solid #f4d4d4 ;
            }
            .orders_c_two {
              border-right:1px solid #f4d4d4 ;
              width: 130px;
            }
            .orders_c_three {
              width: 70px;
              button {
                display: block;
                margin: 6px auto;
                width: 56px;
                height: 30px;
                font-size: 14px;
                border-radius: 15px;
                color: #fff;
                background-color: #ff0000;
              }
            }
          }
        }
      }
      .chat-red-pack,.chat-user-card {
        cursor: pointer;
        display: inline-block;
        margin-left: 10px;
        .red-envelopes {
          position: relative;
          display: flex;
          padding-top: 12px;
          padding-left: 5px;
          height: 78px;
          width: 240px;
          text-align: left;
          background-color: #FA9D3D;
          border-top-left-radius: 5px ;
          border-top-right-radius: 5px ;
          .header_avatar{
            width: 48px;
            height: 48px;
            margin-top: 9px;
          }
          &:before{
            content: " ";
            position: absolute;
            top: 12px;
            right: 100%;
            border: 6px solid transparent;
            border-right-color: #FA9D3D;
          }
          .red-envelopes-message {
            padding-left: 5px;
            color:#f6f6f6;
            div{
              font-size: 16px;
              font-weight: 500;
              width: 180px;
              margin-top: 12px;
            }
            span {
              font-size: 13px;
            }
          }
          .nn-niu {
            margin-left: 10px;
            .nn-niu-vs {
              font-size: 17px;
              color:#CD7F32;
              position: absolute;
              left: 110px;
              top: 56px;
            }
          }

        }
        >p,.nndiv{
          height: 22px;
          line-height: 22px;
          padding-left: 15px;
          font-size: 12px;
          text-align: left;
          background-color: #fff;
          border-bottom-left-radius: 5px;
          border-bottom-right-radius: 5px;
        }
        >.nndiv{
          padding: 0 10px;
          span {
            color:#ff0000;
          }
        }

      }
      .card {
        .red-envelopes {
          background-color: #fff;
          padding-left: 12px;
          border-bottom: 1px solid #e7e7e7;
          .red-envelopes-message{
            color:#333;
            span {
              color:#666;
            }
          }
          &:before{
            border-right-color: #fff;
          }
        }
      }

    }
    .self {
      .handleConfig{
        right: 40px !important;
        left: auto !important;
        text-align: center;
      }
      text-align: right;
      .chat-username {
        right: 67px;
      }
      .avatar {
        float: right;
        margin:0 15px;
      }
      .content {
        background-color: #9eea6a;
        &:before {
          right: -12px;
          vertical-align: middle;
          border-right-color: transparent;
          border-left-color:#9eea6a;
        }
      }
      .resendMsg{
        position: absolute;
        left: -22px;
        top: 8px;
      }
      .chat-content-dom {
        .chat-loading {
          left: -20px;
          right: auto;
        }
      }
      .chat-red-pack {
        .red-envelopes {
          &:before{
            content: " ";
            position: absolute;
            top: 12px;
            left: 100%;
            border: 6px solid transparent;
            border-left-color: #F8AF50;
          }
        }
        p {
          text-align: left;
        }
      }
      .card {
        .red-envelopes {
          &:before{
            content: " ";
            position: absolute;
            top: 12px;
            left: 100%;
            border: 6px solid transparent;
            border-left-color: #fff;
          }
        }
      }

    }

  }
}
