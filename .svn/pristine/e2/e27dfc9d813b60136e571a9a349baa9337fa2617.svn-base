//覆盖element-ui原样式
.wx-login{ //登陆
  .el-form-item{
    margin-top: 40px;
    .el-form-item__content{
      display: flex;
      justify-content: flex-start;
      width: 100%;
      .icon{
        width: 14%;
        background: #333!important;
        text-align: center;
        height: 40px;
        i{
          font-size: 22px;
          color: #fff;
        }
      }
      .el-input__inner{
        border-radius: 0;
        border-left: none;
      }
    }
    .el-button{
      width: 100%;
      background: #333;
      border-color: #333;
      font-size: 16px;
      font-weight: bold;
      border-radius: 0;
    }
    &.gender{
      margin: 0;
      font-size: 16px;
      .el-form-item__label{
        width: 140px;
        color: #333;
        font-size: 16px;
      }
      .el-form-item__content{
        padding-top: 12px;
        .el-radio__label{
          color: #333;
          font-size: 16px;
        }
      }
    }
  }
}

.editor-box{
  .el-cascader{
    width: 100%;
  }
}
.resetpwd-wrap{
  .el-dialog__body{
    padding:0 20px 15px;
  }
  .resetpwd-box {
    .el-form-item{
      margin-top: 30px;
      display: flex;
      justify-content: space-between;
      .el-form-item__label{
        width: 20%;
        text-align: right;
        padding: 0;
      }
      .el-form-item__content{
